{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "preserve",
    "module": "commonjs", 
    "resolveJsonModule": true,  
    "declaration": true,
    "inlineSourceMap": true,
    "outDir": "./dist/",
    "esModuleInterop": true,     
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
